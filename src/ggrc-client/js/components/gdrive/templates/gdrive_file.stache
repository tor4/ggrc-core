<button
  class="{{link_class}} btn btn-small btn-white"
  type="button"
  {{#isInactive}}disabled{{/isInactive}}
  {{#if tooltip}}
    rel="tooltip"
    data-placement="bottom"
    data-original-title="{{tooltip}}"
  {{/if}}
  on:el:click="onClickHandler"
  on:el:keyup="onKeyup(%element, %event)">
  <content></content>
</button>
